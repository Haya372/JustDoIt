<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myApp.Justdoit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.myApp.Justdoit.EditActivity"
            android:parentActivityName="com.myApp.Justdoit.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/set_edit"
            >
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.myApp.Justdoit.MainActivity" />
        </activity>
        <activity
            android:name="com.myApp.Justdoit.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5802533135593818~7073786726"/>
    </application>

</manifest>